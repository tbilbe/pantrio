        "@aws-sdk/util-dynamodb": "^3.43.0",
        "@briancullen/aws-textract-parser": "0.0.2",
        "@types/aws-lambda": "^8.10.85",
        "aws-sdk": "^2.1037.0",
        "jest-config": "^27.3.1",
