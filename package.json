        "@types/node": "10.17.27",
        "aws-cdk": "1.127.0",
        "aws-sdk-client-mock": "0.5.6",
        "jest": "^27.0.7",
        "jest-mock-extended": "^2.0.4",
        "ts-jest": "^27.0.7",
        "ts-node": "^9.0.0",
        "typescript": "~3.9.7"
